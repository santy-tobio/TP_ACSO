    "def get_hex(start, end):\n",
    "    num = 0\n",
    "    for i in range(start, end+1):\n",
    "        num |= 1 << i\n",
    "    return hex(num & 0xFFFFFFFF).upper().replace('0X', '0x')\n",
    "\n",
    "print(get_hex(5,9))"
   ]
  }
